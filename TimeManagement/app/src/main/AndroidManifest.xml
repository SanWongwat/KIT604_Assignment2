<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="assignment2.kit607.timemanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddActivity"
            android:label="Add New Task">
        </activity>
        <activity android:name=".AddUnitCodeActivity"
            android:label="Add New Unitcode">
        </activity>
        <activity android:name=".ViewAllTaskActivity"
            android:label="Tasks">
        </activity>
        <activity android:name=".ViewTaskDetailActivity"
            android:label="Task Detail">
        </activity>
    </application>

</manifest>